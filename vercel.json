{
    "builds": [
    {
        "src": "massmailer/wsgi.py",
        "use": "@vercel/python"
    },
    {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
        "distDir": "staticfiles"
        }
    }
    ],
    "routes": [
    {
        "src": "/(.*)",
        "dest": "/massmailer/wsgi.py"
    },
    {
        "src": "/static/(.*)",
        "dest": "/static/$1"
    }
    ]
}